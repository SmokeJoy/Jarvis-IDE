{
  "name": "jarvis-ide",
  "displayName": "Jarvis IDE MAS",
  "description": "Assistente AI per sviluppatori con supporto import/export sessioni",
  "version": "1.0.0-mas",
  "type": "commonjs",
  "publisher": "jarvis-ide",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jarvis-ide/jarvis-ide"
  },
  "engines": {
    "vscode": "^1.85.0",
    "node": ">=18"
  },
  "author": {
    "name": "Jarvis IDE Team"
  },
  "homepage": "https://github.com/jarvis-ide/jarvis-ide",
  "categories": [
    "Other"
  ],
  "keywords": [
    "ai",
    "llm",
    "openrouter",
    "ollama",
    "lmstudio",
    "chat",
    "development",
    "assistant"
  ],
  "activationEvents": [
    "onCommand:jarvis-ide.start",
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test:providers": "tsx scripts/test-providers.ts",
    "test:providers:openai": "tsx scripts/test-providers.ts openai",
    "test:providers:groq": "tsx scripts/test-providers.ts groq",
    "test:providers:anthropic": "tsx scripts/test-providers.ts anthropic",
    "analyze:results": "tsx scripts/analyze-results.ts",
    "benchmark:import": "ts-node src/scripts/benchmark-import.ts",
    "benchmark:analyze": "tsx scripts/analyze-benchmark.ts",
    "benchmark:test-import": "tsx scripts/quick-test.ts && tsx scripts/import-benchmark.ts",
    "mcp-server": "node --experimental-specifier-resolution=node --loader ts-node/esm src/services/mcp/run-server.ts",
    "mcp-server:debug": "node --trace-warnings --experimental-specifier-resolution=node --loader ts-node/esm src/services/mcp/run-server.ts",
    "test:mcp": "vitest --config=vitest.config.cjs src/services/mcp/__tests__/mcp-server.test.js --detectOpenHandles",
    "test:mcp:coverage": "vitest --config=vitest.config.cjs src/services/mcp/__tests__/mcp-server.test.js --coverage --detectOpenHandles",
    "fix-imports": "ts-node scripts/fix-js-imports.ts",
    "fix-imports:dry": "node scripts/fix-imports.js --dry-run",
    "fix-imports:ts": "node --loader ts-node/esm src/scripts/fix-imports.ts",
    "fix-double-js": "node scripts/fix-double-js.js",
    "schema:webview": "ts-node scripts/generate-schema.ts",
    "schema:all": "ts-node scripts/generate-schema-extended.ts",
    "test:payload": "ts-node scripts/test-payloads.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "fix:imports": "ts-node -P tsconfig.node.json scripts/fix-imports.ts",
    "test:codemods": "vitest run",
    "test:codemods:watch": "vitest watch",
    "changelog:imports": "ts-node scripts/codemods/generate-import-fix-changelog.ts",
    "fix:imports:ast": "ts-node scripts/codemods/fix-import-type-ast.ts",
    "verify:imports": "node scripts/verify-imports.js",
    "verify:all": "pnpm verify:imports && pnpm lint && pnpm tsc --noEmit && pnpm vitest run",
    "type:analyze": "ts-node scripts/type-analyzer.ts",
    "type:migrate": "ts-node -P tsconfig.node.json scripts/type-migrate.ts",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "lint:all": "pnpm lint && pnpm format",
    "fix": "pnpm lint && pnpm format && pnpm type:analyze && pnpm refactor:report",
    "refactor:compile": "tsc -p tsconfig.scripts.json",
    "refactor:fix-nested": "pnpm refactor:compile && node dist-scripts/fixes/fix-nested-safe-message.js",
    "refactor:add-imports": "pnpm refactor:compile && node dist-scripts/fixes/add-missing-imports.js",
    "refactor:chat": "pnpm refactor:compile && node dist-scripts/fixes/transform-chat-message.js",
    "refactor:check-safety": "pnpm refactor:compile && node dist-scripts/checks/verify-all-safety.js",
    "refactor:fix-all": "pnpm refactor:fix-nested && pnpm refactor:add-imports && pnpm refactor:check-safety",
    "refactor:badge": "pnpm refactor:compile && node dist-scripts/dashboard/badge-generator.js",
    "refactor:report": "pnpm refactor:compile && node dist-scripts/dashboard/generate-report.js",
    "refactor:verify": "pnpm refactor:compile && node dist-scripts/checks/verify-safe-messages.js",
    "refactor:all": "pnpm refactor:verify && pnpm refactor:fix-nested && pnpm refactor:add-imports && pnpm tsc --noEmit && pnpm refactor:report",
    "dashboard": "ts-node --esm tools/run-dashboard.ts",
    "dashboard:trend": "ts-node --esm tools/jarvis-refactor.ts --generate-trend",
    "dashboard:detailed": "ts-node --esm tools/jarvis-refactor.ts --detailed-report",
    "dashboard:fix": "ts-node --esm tools/jarvis-refactor.ts --auto-fix",
    "dashboard:priority": "ts-node --esm tools/jarvis-refactor.ts --priority-report",
    "refactor:js-imports": "ts-node --esm -P tsconfig.node.json tools/jarvis-refactor.ts --find-js-imports",
    "refactor:rename-js": "ts-node --esm -P tsconfig.node.json tools/jarvis-refactor.ts --rename-js-to-ts",
    "refactor:map": "ts-node --esm -P tsconfig.node.json tools/jarvis-refactor.ts --generate-map",
    "type:audit": "pnpm type:analyze && pnpm refactor:report && pnpm refactor:js-imports",
    "dashboard:dev": "vite --config refactor-dashboard/vite.config.ts refactor-dashboard",
    "audit:refactor": "ts-node scripts/refactor-assistant.ts"
  },
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "aiDeveloper",
          "title": "AI Developer",
          "icon": "assets/icons/icon.svg"
        },
        {
          "id": "jarvis-ide",
          "title": "Jarvis IDE",
          "icon": "resources/icon.svg"
        },
        {
          "id": "jarvis-refactor",
          "title": "Jarvis Refactor",
          "icon": "assets/icons/refactor.svg"
        }
      ]
    },
    "views": {
      "aiDeveloper": [
        {
          "id": "chatView",
          "name": "Chat",
          "type": "webview"
        }
      ],
      "jarvis-ide": [
        {
          "type": "webview",
          "id": "jarvis.settingsView",
          "name": "Impostazioni"
        }
      ],
      "jarvis-refactor": [
        {
          "type": "webview",
          "id": "jarvis.refactorOverlay",
          "name": "Refactor Tracker",
          "visibility": "visible"
        }
      ]
    },
    "commands": [
      {
        "command": "jarvis-ide.start",
        "title": "Avvia Jarvis IDE"
      },
      {
        "command": "jarvis-ide.openInNewTab",
        "title": "Apri Jarvis IDE in una Nuova Scheda"
      },
      {
        "command": "jarvis-ide.setLogLevel",
        "title": "Imposta Livello Log"
      },
      {
        "command": "jarvis-ide.exportLogFile",
        "title": "Esporta Log Corrente"
      },
      {
        "command": "jarvis-ide.openLogFolder",
        "title": "Apri Cartella Log"
      },
      {
        "command": "jarvis-ide.getSystemPrompt",
        "title": "Ottieni System Prompt"
      },
      {
        "command": "jarvis-ide.saveSystemPrompt",
        "title": "Salva System Prompt"
      },
      {
        "command": "jarvis-ide.updateSetting",
        "title": "Aggiorna Impostazione"
      },
      {
        "command": "jarvis-ide.readFile",
        "title": "Leggi Contenuto File"
      },
      {
        "command": "jarvis-ide.editFile",
        "title": "Modifica File"
      },
      {
        "command": "jarvis-ide.createFile",
        "title": "Crea Nuovo File"
      },
      {
        "command": "jarvis-ide.deleteFile",
        "title": "Elimina File"
      },
      {
        "command": "jarvis-ide.listFiles",
        "title": "Elenca File in Directory"
      },
      {
        "command": "jarvis-ide.listFilesRecursive",
        "title": "Elenca File Ricorsivamente"
      },
      {
        "command": "jarvis.readSystemPrompt",
        "title": "Jarvis: Leggi System Prompt"
      },
      {
        "command": "jarvis.saveSystemPrompt",
        "title": "Jarvis: Salva System Prompt"
      },
      {
        "command": "jarvis-ide.openSystemPromptFile",
        "title": "Jarvis-IDE: Apri File System Prompt"
      },
      {
        "command": "jarvis-ide.setSystemPromptPath",
        "title": "Jarvis-IDE: Imposta Percorso System Prompt"
      },
      {
        "command": "jarvis.runAgent",
        "title": "Jarvis: Esegui Agente Autonomo"
      },
      {
        "command": "jarvis.analyzeFile",
        "title": "Jarvis: Analizza File Corrente"
      },
      {
        "command": "jarvis-ide.mas.saveQueueNow",
        "title": "MAS: Salva Task Queue"
      },
      {
        "command": "jarvis-ide.mas.exportState",
        "title": "MAS: Esporta Task Queue"
      },
      {
        "command": "jarvis-ide.mas.importState",
        "title": "MAS: Importa Task Queue"
      },
      {
        "command": "jarvis-ide.mas.showTaskQueue",
        "title": "MAS: Visualizza Task Queue"
      },
      {
        "command": "jarvis-ide.mas.addInstruction",
        "title": "MAS: Aggiungi Istruzione"
      },
      {
        "command": "jarvis-ide.mas.abortTask",
        "title": "MAS: Annulla Task"
      },
      {
        "command": "jarvis-ide.mas.toggleAgent",
        "title": "MAS: Attiva/Disattiva Agente"
      },
      {
        "command": "jarvis.importSession",
        "title": "Jarvis: Importa Sessione di Chat"
      },
      {
        "command": "jarvis.exportSession",
        "title": "Jarvis: Esporta Sessione di Chat"
      },
      {
        "command": "jarvis.validateSession",
        "title": "Jarvis: Valida Sessione di Chat"
      },
      {
        "command": "jarvis.convertFormat",
        "title": "Jarvis: Converti Formato Sessione"
      },
      {
        "command": "jarvis.previewSession",
        "title": "Jarvis: Anteprima Sessione"
      },
      {
        "command": "jarvis.openRefactorDashboard",
        "title": "Jarvis: Apri Dashboard Refactoring",
        "category": "Jarvis Refactor"
      },
      {
        "command": "jarvis.refreshRefactorData",
        "title": "Jarvis: Aggiorna Dati Refactoring",
        "category": "Jarvis Refactor"
      },
      {
        "command": "jarvis.showCriticalFiles",
        "title": "Jarvis: Mostra File Critici",
        "category": "Jarvis Refactor"
      }
    ],
    "configuration": {
      "title": "Jarvis IDE",
      "properties": {
        "jarvis-ide.provider": {
          "type": "string",
          "default": "openai",
          "enum": [
            "openai",
            "openrouter",
            "local"
          ],
          "description": "Provider LLM da utilizzare"
        },
        "jarvis-ide.use_docs": {
          "type": "boolean",
          "default": false,
          "description": "Abilita l'uso della documentazione durante l'analisi del codice"
        },
        "jarvis-ide.coder_mode": {
          "type": "boolean",
          "default": true,
          "description": "Abilita la modalit√† coder per ottimizzare le risposte per il codice"
        },
        "jarvis-ide.contextPrompt": {
          "type": "string",
          "default": "",
          "description": "Prompt di contesto personalizzato da utilizzare nelle conversazioni"
        },
        "jarvis-ide.refactor.autoScanOnOpen": {
          "type": "boolean",
          "default": true,
          "description": "Esegui automaticamente una scansione del codice all'apertura dell'editor"
        },
        "jarvis-ide.refactor.showDecorations": {
          "type": "boolean",
          "default": true,
          "description": "Mostra decorazioni nell'editor per 'any' e import .js"
        },
        "jarvis-ide.refactor.refreshInterval": {
          "type": "number",
          "default": 300,
          "description": "Intervallo in secondi per l'aggiornamento automatico dei dati di refactoring"
        }
      }
    }
  },
